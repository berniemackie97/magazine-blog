---
const isProd = import.meta.env.PROD;
let AnalyticsComponent = null;

if (isProd) {
  try {
    const mod = await import(/* @vite-ignore */ '@vercel/analytics/astro');
    AnalyticsComponent = mod.Analytics ?? mod.default ?? null;
  } catch {
    AnalyticsComponent = null;
  }
}
---

{AnalyticsComponent ? <AnalyticsComponent /> : null}
